 pipeline {
    agent any 
    
    tools{
        jdk 'jdk11'
        maven 'maven3'
    }
    stages{
        
        stage("Git Checkout"){
            steps{
                git branch: 'main', url: 'https://github.com/vishwanath0303/Jenkins-Zero-To-Hero.git'
            }
        }
        
        stage("Maven Build"){
            steps{
                sh "mvn clean install"
            }
}
}
}
